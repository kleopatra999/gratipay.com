from aspen.http import status_strings
[---]
banner = "401"
[---] text/html via jinja2
{% extends "templates/base.html" %}
{% block content %}
<p>{{ _('Please sign in to continue.') }}</p>
<p>{{ sign_in_using() }}</p>
{% endblock %}
